<script type='text/javascript'>
$(function() {
Dashing.gridsterLayout('[{"col":1,"row":3},{"col":3,"row":2},{"col":2,"row":2},{"col":1,"row":1},{"col":3,"row":1},{"col":1,"row":2}]')
